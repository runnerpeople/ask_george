<script type="text/javascript">
    function like(button1,button2,value) {
        document.getElementById(value).innerHTML = ++document.getElementById(button1).value;
        document.getElementById(button2).value = document.getElementById(button1).value;
    }
    function dislike(button1,button2,value) {
        document.getElementById(value).innerHTML = --document.getElementById(button1).value;
        document.getElementById(button2).value = document.getElementById(button1).value;
    }
</script>

<script type="text/javascript">
    $(function () {
        var ul = $('.dropdown-toggle ul')
        $('.dropdown-toggle a, #close').click(function () {
            ul.slideToggle();
        })
        $('html').click(function (event) {
            if($(event.target).parents(".dropdown-toggle").size()) return;
            ul.slideUp();
        });
    });
</script>
